{
  "name": "flights",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm test:unit",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --config ./test/jest-unit.json",
    "test:unit:verbose": "jest --config ./test/jest-unit.json --verbose",
    "test:unit:watch": "jest --config ./test/jest-unit.json --watch",
    "test:unit:cov": "jest --config ./test/jest-unit.json --coverage",
    "test:unit:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-unit.json --runInBand",
    "test:integration": "dotenv -e .env.test -- jest --config ./test/jest-integration.json",
    "test:integration:verbose": "dotenv -e .env.test -- jest --config ./test/jest-integration.json --verbose",
    "test:integration:watch": "dotenv -e .env.test -- jest --config ./test/jest-integration.json --watch",
    "test:integration:cov": "dotenv -e .env.test -- jest --config ./test/jest-integration.json --coverage",
    "test:integration:debug": "dotenv -e .env.test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-integration.json --runInBand",
    "test:e2e": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json",
    "test:e2e:verbose": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json --verbose",
    "test:e2e:watch": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json --watch",
    "test:e2e:cov": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json --coverage",
    "test:e2e:debug": "dotenv -e .env.test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-e2e.json --runInBand",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio"
  },
  "dependencies": {
    "@fastify/static": "^8.2.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.10.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.0.0",
    "dotenv-cli": "^8.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "supertest": "^7.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "fastify": "^5.4.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.10.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=22.13.0"
  }
}
